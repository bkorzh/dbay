{
  "productName": "Device Bay",
  "version": "0.1.0",
  "identifier": "device.bay",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 800,
        "resizable": false,
        "title": "Device Bay",
        "width": 438
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "macOS": {
      "files": {
        "Frameworks": "python_binary/_internal"
      }
    },
    "active": true,
    "targets": "all",
    "icon": [
      "icons/db_2.png"
    ],
    "resources": [
          "python_binary/_internal/*"
      ],
    "externalBin": [
        "python_binary/main"
      ]
  },
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json"
}
