{
  "productName": "Device Bay",
  "version": "0.1.0",
  "identifier": "device.bay",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 800,
        "resizable": false,
        "title": "Device Bay",
        "width": 438
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "linux": {
      "deb": {
        "files": {
          "usr/lib/device-bay/_internal": "python_binary/_internal/",
          "usr/lib/device-bay/main": "python_binary/main-x86_64-unknown-linux-gnu",
          "usr/bin/_internal": "python_binary/_internal/"
        }
      }
    },
    "macOS": {
      "files": {
        "Frameworks": "python_binary/_internal"
      }
    },
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "python_binary/_internal/*"
    ],
    "externalBin": [
      "python_binary/main"
    ]
  },
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json"
}